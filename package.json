{
  "name": "david-harting-personal-site",
  "private": true,
  "description": "David Harting's personal website",
  "version": "0.1.0",
  "author": "David Harting <davidhartingdev@gmail.com>",
  "bugs": {
    "url": "https://github.com/davidharting/personal-site/issues"
  },
  "dependencies": {
    "gatsby": "2.15.9",
    "gatsby-image": "^2.0.22",
    "gatsby-plugin-feed": "^2.0.8",
    "gatsby-plugin-google-analytics": "^2.0.5",
    "gatsby-plugin-manifest": "^2.0.5",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.6",
    "gatsby-plugin-typography": "^2.2.0",
    "gatsby-remark-autolink-headers": "^2.0.16",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-images": "^2.0.4",
    "gatsby-remark-prismjs": "^3.0.0",
    "gatsby-remark-responsive-iframe": "^2.0.5",
    "gatsby-remark-smartypants": "^2.0.5",
    "gatsby-source-filesystem": "^2.0.2",
    "gatsby-transformer-remark": "^2.1.6",
    "gatsby-transformer-sharp": "^2.1.3",
    "prismjs": "^1.15.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.13",
    "typeface-merriweather": "0.0.43",
    "typeface-montserrat": "0.0.43",
    "typography": "^0.16.17",
    "typography-theme-st-annes": "^0.16.18"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "2.1.0",
    "@typescript-eslint/parser": "2.1.0",
    "babel-eslint": "10.0.3",
    "eslint": "6.3.0",
    "eslint-config-react-app": "5.0.1",
    "eslint-plugin-flowtype": "4.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "2.0.1",
    "prettier": "1.14.2"
  },
  "homepage": "https://www.davidharting.com",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidharting/personal-site.git"
  },
  "scripts": {
    "build": "gatsby build",
    "bust-cache": "aws cloudfront create-invalidation --profile s3-deployer --distribution-id E13FMPKR54AMQM --paths /index.html",
    "dev": "gatsby develop",
    "deploy": "npm run build && npm run sync && npm run bust-cache",
    "develop": "gatsby develop",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "sync": "aws s3 sync ./public s3://www.davidharting.com/ --profile s3-deployer --acl public-read",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  }
}
